import java.util.*;
class Project {
    public static void main(String args[])
    {
        int i,count=0,n,r,t;
        Scanner s=new Scanner(System.in);
        System.out.println("Enter the number ");
        n=s.nextInt();
        t=n;
        while(n>0){
            n=n/10;
            count++;
        }
        System.out.println("number of digit in given number is="+count);
    }
}
