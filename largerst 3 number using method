class Project{
   static int max(int x,int y,int z)
    {
        if(x >y &&  x>z)
            return x;
        else if(y>z)
            return y;
        else
            return z;
    }
    public static void main(String args[])
    {
        int max,a=121,b=59,c=523;
       max= max(a,b,c);
        System.out.println(max);
    }
}
