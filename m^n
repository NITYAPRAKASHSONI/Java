import java.util.*;
public class Project{
   static double pow(double m ,double n)
    {
        if(m==1)
            return 0;
        if(n==1)
            return 1;
        else
            return pow(m,n-1)*m;
                
        
           
       
    }
   

    public static void main(String args[])
    {
        double m,n;
        System.out.println("Enter the two  number for m is to n ");
        Scanner s=new Scanner(System.in);
        m=s.nextDouble();
        n=s.nextDouble();
        System.out.println(pow(m,n));
    
        
    }
}
