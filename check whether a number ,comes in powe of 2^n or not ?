import java.util.*;
public class Java{
   static int raise(int n)
    {
        int i,t;
        t=n;
        for(i=0;i<=n/2;i++)
        {
            if(Math.pow(2, i)==t)
                return i;
        }
        return -1;
        
    }
  
     static public  void main(String args[])
    {
       int n;
        Scanner s=new Scanner (System.in);
        System.out.println("enter the number");
        n=s.nextInt();
      
        System.out.println(raise(n));
    }
}
