import java.util.*;
public class Project{
   static int fact(int n)
    {
        if(n<=1)
            return 1;
        else
            return fact(n-1)*n;
       
    }
   

    public static void main(String args[])
    {
        int n=10;
        System.out.println(fact(n));
    
        
    }
}
